ifndef config
  config=1998
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),1998)
  CoverageTest_config = 1998
endif
ifeq ($(config),2017)
  CoverageTest_config = 2017
endif
ifeq ($(config),macro)
  CoverageTest_config = macro
endif
ifeq ($(config),b_macro)
  CoverageTest_config = b_macro
endif

PROJECTS := CoverageTest

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CoverageTest:
ifneq (,$(CoverageTest_config))
	@echo "==== Building CoverageTest ($(CoverageTest_config)) ===="
	@${MAKE} --no-print-directory -C . -f CoverageTest.make config=$(CoverageTest_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CoverageTest.make clean

help:
