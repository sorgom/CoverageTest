# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=ci
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),ci)
  CoverageTest_config = ci
endif
ifeq ($(config),debug)
  CoverageTest_config = debug
endif

PROJECTS := CoverageTest

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CoverageTest:
ifneq (,$(CoverageTest_config))
	@echo "==== Building CoverageTest ($(CoverageTest_config)) ===="
	@${MAKE} --no-print-directory -C . -f CoverageTest.make config=$(CoverageTest_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CoverageTest.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  ci"
	@echo "  debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   CoverageTest"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"