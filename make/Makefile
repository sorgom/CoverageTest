ifndef config
  config=standard
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),standard)
  CoverageTest_config = standard
endif
ifeq ($(config),mod_cpp)
  CoverageTest_config = mod_cpp
endif
ifeq ($(config),macro)
  CoverageTest_config = macro
endif
ifeq ($(config),b_macro)
  CoverageTest_config = b_macro
endif
ifeq ($(config),fd_set)
  CoverageTest_config = fd_set
endif
ifeq ($(config),exclude)
  CoverageTest_config = exclude
endif
ifeq ($(config),count)
  CoverageTest_config = count
endif

PROJECTS := CoverageTest

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CoverageTest:
ifneq (,$(CoverageTest_config))
	@echo "==== Building CoverageTest ($(CoverageTest_config)) ===="
	@${MAKE} --no-print-directory -C . -f CoverageTest.make config=$(CoverageTest_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CoverageTest.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  standard"
